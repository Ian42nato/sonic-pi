use_bpm 160
use_synth :blade
use_synth_defaults amp: 0.5

define :stranger_things_theme do
  play :c2
  sleep 0.5
  play :e2
  sleep 0.5
  play :g2
  sleep 0.5
  play :b2
  sleep 0.5
  
  #secong 4 notes
  play :c3
  sleep 0.5
  play :b2
  sleep 0.5
  play :g2
  sleep 0.5
  play :e2
  sleep 0.5
end



with_fx :bitcrusher do
  stranger_things_theme
end

with_fx :whammy do
  stranger_things_theme
end

with_fx :compressor do
  stranger_things_theme
end

with_fx :krush do
  stranger_things_theme
end
